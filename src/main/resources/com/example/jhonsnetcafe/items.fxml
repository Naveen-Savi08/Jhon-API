<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.jhonsnetcafe.HelloController">
   <children>
      <Pane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #89cce9;">
         <children>
            <Pane prefHeight="600.0" prefWidth="200.0" style="-fx-background-color: #fcfcf4;">
               <children>
                  <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="25.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@Green and Purple Simple Creative Logo.png" />
                     </image>
                  </ImageView>
                  <Button fx:id="onClickDealers" layoutX="65.0" layoutY="379.0" mnemonicParsing="false" onAction="#onClickDealers" prefHeight="52.0" prefWidth="121.0" style="-fx-background-color: #ece0d0;" text="Dealers">
                     <font>
                        <Font name="System Bold" size="25.0" />
                     </font>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
                  <Button layoutX="65.0" layoutY="457.0" mnemonicParsing="false" prefHeight="52.0" prefWidth="121.0" style="-fx-background-color: #ece0d0;" text="Exit">
                     <font>
                        <Font name="System Bold" size="25.0" />
                     </font>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
                  <ImageView fitHeight="52.0" fitWidth="52.0" layoutX="12.0" layoutY="379.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@icons8-dealer-100.png" />
                     </image>
                  </ImageView>
                  <ImageView fitHeight="52.0" fitWidth="52.0" layoutX="9.0" layoutY="456.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@icons8-exit-48 (1).png" />
                     </image>
                  </ImageView>
               </children>
            </Pane>
            <Pane layoutX="230.0" layoutY="50.0" prefHeight="50.0" prefWidth="527.0" style="-fx-background-color: #dbe3f6; -fx-background-radius: 20;">
               <children>
                  <Button fx:id="onClickAddItem" layoutX="8.0" layoutY="7.0" mnemonicParsing="false" onAction="#onClickAddItem" prefHeight="32.0" prefWidth="68.0" style="-fx-background-color: #367caf; -fx-background-radius: 20;" text="Add">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
                  <Button fx:id="onClickDeleteItem" layoutX="150.0" layoutY="7.0" mnemonicParsing="false" onAction="#onClickDeleteItem" prefHeight="36.0" prefWidth="76.0" style="-fx-background-color: #367caf; -fx-background-radius: 20;" text="Delete">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
                  <Button fx:id="onClickUpdateItem" layoutX="300.0" layoutY="7.0" mnemonicParsing="false" onAction="#onClickUpdateItem" prefHeight="36.0" prefWidth="76.0" style="-fx-background-color: #367caf; -fx-background-radius: 20;" text="Update">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
                  <Button fx:id="onClickViewItem" layoutX="452.0" layoutY="7.0" mnemonicParsing="false" onAction="#onClickViewItem" prefHeight="32.0" prefWidth="68.0" style="-fx-background-color: #367caf; -fx-background-radius: 20;" text="View">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
               </children>
            </Pane>
         </children>
      </Pane>
   </children>
</AnchorPane>
