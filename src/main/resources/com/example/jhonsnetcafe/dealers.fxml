<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.jhonsnetcafe.HelloController">
   <children>
      <Pane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #89cce9;">
         <children>
            <Pane prefHeight="600.0" prefWidth="200.0" style="-fx-background-color: #fcfcf4;">
               <children>
                  <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="25.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@Green and Purple Simple Creative Logo.png" />
                     </image>
                  </ImageView>
                  <Button fx:id="onClickBackToMenu" layoutX="42.0" layoutY="467.0" mnemonicParsing="false" onAction="#onClickBackToMenu" prefHeight="50.0" prefWidth="150.0" style="-fx-background-color: #ece0d0;" text="Back">
                     <font>
                        <Font name="System Bold" size="25.0" />
                     </font>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
                  <ImageView fitHeight="35.0" fitWidth="45.0" layoutX="7.0" layoutY="477.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@icons8-back-48.png" />
                     </image>
                  </ImageView>
               </children>
            </Pane>
            <Pane layoutX="230.0" layoutY="50.0" prefHeight="50.0" prefWidth="527.0" style="-fx-background-color: #dbe3f6;">
